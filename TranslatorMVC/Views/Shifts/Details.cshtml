@model TranslatorMVC.Models.Shift

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Shift</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
         Shift Start
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Shift_Start)
        </dd>
        <dt class = "col-sm-2">
           Shift End
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Shift_End)
        </dd>
        <dt class = "col-sm-2">
            Employee
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.EmployeeID)
        </dd>
        <dt class = "col-sm-2">
            Capacity
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Emp_Capacity)
        </dd>
        <dt class = "col-sm-2">
           Created Date
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Shift_CreateDate)
        </dd>
        <dt class = "col-sm-2">
            Active
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Shift_Active)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" class="btn btn-primary" asp-route-id="@Model?.ShiftID">Edit</a> |
    <a asp-action="Index" class="btn btn-secondary">Back to List</a>
